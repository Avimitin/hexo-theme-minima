<h1 class="post-title"><%- page.title %></h1>

<% if (page.tldr) { %>
	<p class="post-tldr">
		<span id="tldr-start">TL;DR:</span> <%-page.tldr%>
	</p>
<% } %>

<% if (page.thumbnail) { %>
	<img src="<%- page.thumbnail %>"/>
<% } %>

<% if (page.tnalt) { %>
	<p class="post-tn-alt"><%- page.tnalt %></p>
<% } %>

  <%- page.content%>

  <p><%- list_tags(page.tags, {class: 'classtest', show_count: false, style: false}) %> â€” <%- full_date(page.date, 'll') %></p>
  <%- partial('partial/comments') %>
